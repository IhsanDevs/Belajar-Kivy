<Root>:
    ClockRect:
        pos: 150, 150
    AnimRect:
        pos: 250, 300

<ClockRect>:
    canvas:
        Color:
            rgba: 1, 0, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size

<AnimRect>:
    canvas:
        Color:
            rgba: 1, 1, 0, 1
        Rectangle:
            pos: self.pos
            size: self.size